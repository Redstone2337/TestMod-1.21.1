{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
    "license": "${mod_license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.redstone233.tmb.fabric.FabricTestMod"
        ],
        "client": [
            "net.redstone233.tmb.fabric.client.FabricTestModClient"
        ],
        "fabric-datagen": [
            "net.redstone233.tmb.fabric.client.FabricTestModDataGenerator"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.17.3",
        "minecraft": "~1.21.1",
        "java": ">=21",
        "fabric-api": "*"
    },
    "suggests": {
        "another-mod": "*"
    }
}